<yas>

    <scene root="root" axis_length="5"/>
 
    <views default="view4-perspective" >
         <!-- perspective view 1 -->
        <perspective id="view1-perspective" near="0.1" far="500" angle="20">
            <from x="25" y="25" z="25"/>
            <to x="0" y="-3" z="0"/>
        </perspective>

        <!-- perspective view 2 -->
         <perspective id="view2-perspective" near="0.1" far="500" angle="30">
            <from x="6" y="10" z="6"/>
            <to x="5" y="2" z="5"/>
        </perspective>

        <!-- perspective view 3 -->
         <perspective id="view3-perspective" near="0.1" far="500" angle="30">
            <from x="25" y="25" z="0"/>
            <to x="6" y="3" z="5"/>
        </perspective>

        <!-- perspective view 4 -->
         <perspective id="view4-perspective" near="0.5" far="500" angle="60">
            <from x="5" y="20" z="5"/>
            <to x="6" y="0" z="5"/>
        </perspective>

        <!-- ortho view 1 -->
        <ortho id="view3-ortho" near="0.1" far="500" left="-20" right="20" top="22" bottom="-22">
            <from x="6" y="5" z="30" />
            <to x="6" y="5" z="0" />
        </ortho>
    </views>
 
    <ambient>
        <ambient r="0" g="0" b="0" a="1.0"/>
        <background r="0.34" g="0.69" b="0.96" a="1.0"/>
    </ambient>

    <lights>
        <!-- omni light 1 -->
        <omni id="light 1 (omni)" enabled="1">
            <location x="1" y="13" z="1" w="1"/>
            <ambient r="0.5" g="0.5" b="0.5" a="1.0"/>
            <diffuse r="0.9" g="0.9" b="0.9" a="1"/>
            <specular r="0.6" g="0.7" b="1.0" a="1.0"/>
        </omni>

        <!-- omni light 2 -->
         <omni id="light 2 (omni)" enabled="1">
            <location x="6" y="5" z="8" w="1"/>
            <ambient r="0.6" g="0.6" b="0.6" a="1.0"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0"/>
            <specular r="0.8" g="0.8" b="0.8" a="1.0"/>
        </omni>

        <!-- spot light 1 -->
        <spot id=" lamp light (spot)" enabled="1" angle="10" exponent="2">
            <location x="5.3" y="1.95" z="5" w="1"/>
            <target x="5" y="-5" z="5"/>
             <ambient r="0.1" g="0.1" b="0.1" a="0.1" />
            <diffuse r="0.2" g="0.2" b="0.2" a="0.1" />
            <specular r="1" g="1" b="1" a="0.1" />
        </spot>
    </lights>

     <textures>
        <texture id="walltxt" file="scenes/images/wall.jpg"/>
        <texture id="floortxt" file="scenes/images/wood.jpg"/>
        <texture id="rugtxt" file="scenes/images/rug.jpg"/>
        <texture id="blacktxt" file="scenes/images/black.jpg"/>
        <texture id="whitetxt" file="scenes/images/white.jpg"/>
        <texture id="metaltxt" file="scenes/images/metal.jpg"/>
        <texture id="visortxt" file="scenes/images/visor.jpg"/>
        <texture id="soiltxt" file="scenes/images/soil.jpg"/>
        <texture id="vistatxt" file="scenes/images/vista.jpg"/>
        <texture id="cactustxt" file="scenes/images/cactus.jpg"/>
        <texture id="velourtxt" file="scenes/images/velour.jpg"/>
        <texture id="marbletxt" file="scenes/images/marble.jpg"/>
        <texture id="tabletxt" file="scenes/images/table.jpg"/>
        <texture id="pinktxt" file="scenes/images/pink.jpg"/>
    </textures>

     <materials>
        <material id="standard" shininess="10">
            <emission r="0.2" g="0.2" b="0.2" a="1"/>
            <ambient r="0.5" g="0.5" b="0.5" a="1"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1"/>
            <specular r="0.5" g="0.5" b="0.5" a="1"/>
        </material>

        <material id="white" shininess="10">
            <emission r="0.2" g="0.2" b="0.2" a="1"/>
            <ambient r="0.4" g="0.4" b="0.4" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <specular r="1" g="1" b="1" a="1"/>
        </material>

        <material id="dark" shininess="10">
            <emission r="0.1" g="0.1" b="0.1" a="1"/>
            <ambient r="0.1" g="0.1" b="0.1" a="1"/>
            <diffuse r="0.1" g="0.1" b="0.1" a="1"/>
            <specular r="0.5" g="0.5" b="0.5" a="1"/>
        </material>

        <material id="light" shininess="10">
            <emission  r="0.35" g="0.35" b="0.35" a="1"/>
            <ambient r="0.4" g="0.4" b="0.4" a="1"/>
            <diffuse  r="0.4" g="0.4" b="0.4" a="1"/>
            <specular r="0.8" g="0.8" b="0.8" a="1"/>
        </material>

        <material id="fur" shininess="10">
            <emission r="0.3" g="0.3" b="0.3" a="1"/>
            <ambient r="0.5" g="0.5" b="0.5" a="1"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1"/>
            <specular r="0.7" g="0.7" b="0.7" a="1"/>
        </material>

        <material id="desklamp" shininess="10">
            <emission r="0.7" g="0.7" b="0.7" a="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="0" g="0" b="0" a="1"/>
            <specular r="0.5" g="0.5" b="0.5" a="1"/>
        </material>

    </materials>

     <transformations>  
        <transformation id="to_origin">
            <translate x="-1.5" y="-1.5" z="0"/>
         </transformation>
    </transformations>

     <primitives>
        <!-- rectangle -->
        <primitive id="MyRectangle">
            <rectangle x1="1" y1="1" x2="2" y2="2"/>
        </primitive>

        <!-- cylinder -->
        <primitive id="MyCylinder">
           <cylinder base="1" top="1" height="1" stacks="1" slices="64"/>
        </primitive>

        <!-- sphere -->
        <primitive id="MySphere">
           <sphere radius="1" stacks="32" slices="64"/>
        </primitive>

        <!-- torus -->
        <primitive id="MyTorus">
             <torus inner="1" outer="2" slices="64" loops="20" />
        </primitive>

        <!-- curved cylinder -->
        <primitive id="MyCurvedCylinder">
             <torus inner="1" outer="1" slices="64" loops="20" />
        </primitive>

        <!-- triangle -->
        <primitive id="MyIqTriangle">
             <triangle x1="1" y1="0" z1="0" x2="-1" y2="0" z2="0" x3="0" y3="0" z3="2"/>
        </primitive>
    </primitives>

     <components>
        <component id="root">
            <transformation>
            </transformation>

            <materials>
                <material id="standard"/>
            </materials>
        
             <texture id="none"/>

            <children>
                <componentref id="wall1"/>
                <componentref id="wall2"/>
                <componentref id="wall3"/>
                <componentref id="wall4"/>
                <componentref id="floor"/>
                <componentref id="rug"/>
                <componentref id="table"/>
                <componentref id="seats"/>
                <componentref id="game"/>
                <componentref id="lamp"/> 
                <componentref id="tv"/>
                <componentref id="visor"/>
                <componentref id="mirrors"/>
                <componentref id="vase"/>
                <componentref id="window"/>
                <componentref id="windowborders"/>
                <componentref id="littletable"/>
                <componentref id="desklamp"/>
                <componentref id="door"/>
            </children>
